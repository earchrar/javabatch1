        Types of => int
        Object = Variable (int = nextLineInt() )

        Scanner sca = new Scanner(System.in);
        System.out.println("Enter your age =  ");
        int age = sca.nextLineInt();
        System.out.println("Age is =  "+age);

        Types of => float
        Object = Variable (float = nextFloat() )

        Scanner sca = new Scanner(System.in);
        System.out.println("Enter First Number is =  ");
        float fnumber = sca.nextFloat();
        System.out.println("Enter Second Number is =  ");
        float snumber = sca.nextFloat();
        float result = fnumber + snumber;
        System.out.println("Result Number is = "+result);

        Types of => char 
        Object = Variable (char = .next().charAt(0))

        Scanner sca = new Scanner(System.in);
        System.out.println("Enter you character = ");
        char a = sca.next().charAt(0);
        System.out.println("Result Character : "+a);

Lesson 2 
 Arithemetic Operators ( Addition(+) , Subtraction(-) , Multiple(*) , Modulue(%) , increment (++) , decrement (--))

 exe 1 
    Addition (+)
    int a = 50;
    int b = 10;
    int result = a + b;
    System.out.print("Result Number is = ",result);

    Subtraction (-)
    int a = 50;
    int b = 10;
    int result = a - b;
    System.out.print("Result Number is = ",result);

    Multiple (*)
    int a = 50;
    int b = 10;
    int result = a * b;
    System.out.print("Result Number is = ",result);

    Modulue (%)
    int a = 50;
    int b = 10;
    int result = a % b;
    System.out.print("Result Number is = ",result);

    Increment (++) 
    int a = 50;
    int b = 10;
    int result = a % b;
    ++result;
    System.out.print("Result Number is = ",result);

    Decrement (--) 
    int a = 50;
    int b = 10;
    int result = a % b;
    --result;
    System.out.print("Result Number is = ",result);

